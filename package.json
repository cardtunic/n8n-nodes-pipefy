{
	"name": "@cardtunic/n8n-nodes-pipefy",
	"version": "0.1.2",
	"description": "A community node that brings (part of) the Pipefy GraphQL API to your n8n instance!",
	"keywords": [
		"n8n-community-node-package",
		"pipefy",
		"n8n",
		"graphql"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "Kaleb Mendes",
		"email": "cardtunic@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cardtunic/n8n-nodes-pipefy.git"
	},
	"engines": {
		"node": ">=20.15"
	},
	"main": "index.js",
	"scripts": {
		"build": "npx rimraf dist && tsc && gulp build:icons",
		"dev": "nodemon --config nodemon.json --exec \"bun run build && n8n start\"",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/PipefyPersonalTokenApi.credentials.js",
			"dist/credentials/PipefyServiceAccountApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Pipefy/Pipefy.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "~8.32.0",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.0",
		"nodemon": "^3.1.10",
		"prettier": "^3.5.3",
		"typescript": "^5.8.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"axios": "^1.11.0"
	}
}
